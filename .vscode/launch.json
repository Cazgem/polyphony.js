    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "name": "EventSub",
            "request": "launch",
            "runtimeArgs": [
                "run-script",
                "debug"
            ],
            "runtimeExecutable": "npm",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "args": [
                "events"
            ],
            "program": "${workspaceFolder}/test/app.js"
        },
        {
            "type": "node",
            "name": "Debug",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "args": [
                "game",
                "Minecraft"
            ],
            "program": "${workspaceFolder}/test/app.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/index.js"
        }
    ]
}